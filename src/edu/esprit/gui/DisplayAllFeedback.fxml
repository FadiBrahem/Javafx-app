<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane id="AnchorPane" prefHeight="661.0" prefWidth="997.0" stylesheets="@dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.gui.DisplayAllFeedbackController">
   <children>
      <ImageView fitHeight="690.0" fitWidth="1001.0" layoutX="-2.0" layoutY="-4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@dashboard.png" />
         </image></ImageView>
      <TableView fx:id="FeedbackTable" layoutX="222.0" layoutY="258.0" prefHeight="244.0" prefWidth="749.0">
         <columns>
            <TableColumn fx:id="descriptionFeedback" prefWidth="406.0" text="Description" />
            <TableColumn fx:id="dateFeedback" prefWidth="183.0" text="Date" />
            <TableColumn fx:id="userFeedback" prefWidth="57.0" text="User" />
            <TableColumn fx:id="offerFeedback" minWidth="0.0" prefWidth="102.0" text="Offer" />
         </columns>
      </TableView>
      <Button fx:id="addFeedbackBtn" layoutX="580.0" layoutY="555.0" mnemonicParsing="false" onAction="#addFeedback" prefHeight="35.0" prefWidth="78.0" text="ADD" />
      <Button fx:id="updateFeedbackBtn" layoutX="457.0" layoutY="553.0" mnemonicParsing="false" onAction="#updateFeedback" prefHeight="37.0" prefWidth="81.0" text="UPDATE" />
      <Button fx:id="removeComplaintBtn" layoutX="234.0" layoutY="553.0" mnemonicParsing="false" onAction="#removeFeedback" prefHeight="35.0" prefWidth="83.0" text="REMOVE" />
      <Button fx:id="statBtn" layoutX="347.0" layoutY="554.0" mnemonicParsing="false" onAction="#statFeedback" prefHeight="35.0" prefWidth="74.0" text="STAT" />
      <Circle fill="#691fff" layoutX="89.0" layoutY="72.0" radius="44.0" stroke="TRANSPARENT" strokeType="INSIDE" />
      <Label layoutX="236.0" layoutY="183.0" style="-fx-font-size: 40;" text="Feedbacks" />
   </children>
</AnchorPane>
